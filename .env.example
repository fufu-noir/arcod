# Qobuz API credentials - required
# Get these from https://github.com/QobuzDL/Qobuz-AppID-Secret-Tool
QOBUZ_API_BASE=https://www.qobuz.com/api.json/0.2/
QOBUZ_APP_ID=
QOBUZ_SECRET=
QOBUZ_AUTH_TOKENS=[]

# App settings
NEXT_PUBLIC_APPLICATION_NAME=Qobuz-DL
NEXT_PUBLIC_GITHUB=https://github.com/QobuzDL/Qobuz-DL

# AWS backend (fill in after running `sam deploy`)
NEXT_PUBLIC_AWS_API_URL=
NEXT_PUBLIC_AWS_CLOUDFRONT_DOMAIN=

# AWS Cognito auth (from your Cognito User Pool)
NEXT_PUBLIC_COGNITO_USER_POOL_ID=
NEXT_PUBLIC_COGNITO_CLIENT_ID=
NEXT_PUBLIC_COGNITO_DOMAIN=

# Lyrics service URL (optional)
NEXT_PUBLIC_LYRICS_SERVICE_URL=

# Tidal API (optional)
TIDAL_AUTH_TOKEN=
TIDAL_COUNTRY_CODE=US

# Proxy settings (leave empty unless needed)
CORS_PROXY=
SOCKS5_PROXY=